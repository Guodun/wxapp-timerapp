page{
    background: #336699;
    font-family: "黑体","宋体"
}
.title{
    color: white;
    font-size: 20px;
    text-align: center;
    background-color: orange;
    box-shadow: 2px 2px 10px #888888;
    margin-bottom: 10px;
}
.desc{
    color: white;
}
.big-circle{
    width: 150px;
    height: 150px;
    position: absolute;
    border-radius: 50%;
    background-color: #99CCFF;
}
.time{
    height: 160px;
}
.right{
    clip: rect(auto,auto,auto,75px);
    background-color: #0099FF;
}
.btnGroup{
    display: flex;
    flex-direction: row;
}
.btn{
    background-color: #0099CC;
    color: white;
}
.circle{
    width: 130px;
    height: 130px;
    border-radius: 50%;
    position: absolute;
    background-color: white;
    margin-left: 10px;
    margin-top: 10px;
}
.circle view{
    margin-left: 50px;
    margin-top: 60px;
    color: black;
    font-size: 20px;
}
.second{
    left:390rpx;
}
.first{
    left:20rpx;
}